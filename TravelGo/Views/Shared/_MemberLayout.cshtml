@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer localizer




<!DOCTYPE html>
<html class="loading" lang="en" data-textdirection="ltr">
@await Component.InvokeAsync("_MemberLayoutHead")
<body class="vertical-layout vertical-menu 2-columns   menu-expanded fixed-navbar" data-open="click" data-menu="vertical-menu" data-color="bg-gradient-x-purple-blue" data-col="2-columns">

    <!-- fixed-top-->
    <nav class="header-navbar navbar-expand-md navbar navbar-with-menu navbar-without-dd-arrow fixed-top navbar-semi-light">
        <div class="navbar-wrapper">
            <div class="navbar-container content">
                <div class="collapse navbar-collapse show" id="navbar-mobile">
                    @await Component.InvokeAsync("_MemberLayoutSearch")
                    @await Component.InvokeAsync("_MemberLayoutLanguages")
                    @await Component.InvokeAsync("_MemberLayoutProfileNavbar")
                  
                </div>
            </div>
        </div>
    </nav>

    <!-- ////////////////////////////////////////////////////////////////////////////-->
    @*@await Component.InvokeAsync("_MemberLayoutSidebar")*@

    <div class="main-menu menu-fixed menu-light menu-accordion    menu-shadow " data-scroll-to-active="true" data-img="theme-assets/images/backgrounds/02.jpg">
    <div class="navbar-header">
        <ul class="nav navbar-nav flex-row">
            <li class="nav-item mr-auto">
                <a class="navbar-brand" href="index.html">
                    <img class="brand-logo" alt="Chameleon admin logo" src="~/chameleon/theme-assets/images/logo/logo.png" />
                    <h3 class="brand-text">Chameleon</h3>
                </a>
            </li>
            <li class="nav-item d-md-none"><a class="nav-link close-navbar"><i class="ft-x"></i></a></li>
        </ul>
    </div>
    <div class="main-menu-content">
        <ul class="navigation navigation-main" id="main-menu-navigation" data-menu="menu-navigation">
            <li class=" nav-item">
                    <a asp-area="Memmber" asp-action="MemmberDashboard" asp-controller="dashboard"><i class="ft-home"></i><span class="menu-title" data-i18n="">@localizer["Dashboard"]</span></a>
            </li>
            <li class=" nav-item">
                <a asp-area="Memmber" asp-action="Index" asp-controller="profile"><i class="ft-pie-chart"></i><span class="menu-title" data-i18n="">@localizer["Profile"]</span></a>
            </li>
      
            <li class=" nav-item">
                <a asp-area="Memmber" asp-action="Index" asp-controller="dashboard"><i class="ft-droplet"></i><span class="menu-title" data-i18n="">Icons</span></a>
            </li>
            <li class=" nav-item">
                <a asp-area="Memmber" asp-action="MyCurrentReservation" asp-controller="reservation"><i class="ft-layers"></i><span class="menu-title" data-i18n=""></span> @localizer["Current Reservations"]</a>
            </li>
            <li class="active">
                <a asp-area="Memmber" asp-action="MyOldReservation" asp-controller="reservation"><i class="ft-box"></i><span class="menu-title" data-i18n=""></span> @localizer["Old Reservations"]</a>
            </li>
            <li class=" nav-item">
                <a asp-area="Memmber" asp-action="MyAppRovalReservation" asp-controller="reservation"><i class="ft-bold"></i><span class="menu-title" data-i18n=""></span> @localizer["Approval Reservations"]</a>
            </li>
            <li class=" nav-item">
                <a asp-area="Memmber" asp-action="index" asp-controller="comment"><i class="ft-bold"></i><span class="menu-title" data-i18n=""></span> @localizer["Comments"]</a>
            </li>
            <li class=" nav-item">
               <a asp-area="Memmber" asp-action="index" asp-controller="destination"><i class="ft-bold"></i><span class="menu-title" data-i18n=""></span>@localizer["Destination list"]</a>
            </li>
            <li class=" nav-item">
                <a asp-area="Memmber" asp-action="index" asp-controller="LastDestinations"><i class="ft-bold"></i><span class="menu-title" data-i18n=""></span>@localizer["Last 4 Destinations"]</a>
            </li>
        
            <li class=" nav-item">
                <a href="https://themeselection.com/demo/chameleon-admin-template/documentation"><i class="ft-book"></i><span class="menu-title" data-i18n="">Documentation</span></a>
            </li>
        </ul>
    </div><a class="btn btn-danger btn-block btn-glow btn-upgrade-pro mx-1" href="https://themeselection.com/products/chameleon-admin-modern-bootstrap-webapp-dashboard-html-template-ui-kit/" target="_blank">Download PRO!</a>
    <div class="navigation-background"></div>
</div>



    <!-- ////////////////////////////////////////////////////////////////////////////-->
    <div class="app-content content">
        <div class="content-wrapper">
            <div class="content-wrapper-before"></div>
            @await Component.InvokeAsync("_MemberLayoutHeaderContent")

            <div class="content-body">
                @RenderBody()
            </div>
        </div>
    </div>
    <!-- ////////////////////////////////////////////////////////////////////////////-->
    @await Component.InvokeAsync("_MemberLayoutFooter")

  
    <!-- BEGIN VENDOR JS-->
    <script src="~/chameleon/theme-assets/vendors/js/vendors.min.js" type="text/javascript"></script>
    <!-- BEGIN VENDOR JS-->
    <!-- BEGIN PAGE VENDOR JS-->
    <!-- END PAGE VENDOR JS-->
    <!-- BEGIN CHAMELEON  JS-->
    <script src="~/chameleon/theme-assets/js/core/app-menu-lite.js" type="text/javascript"></script>
    <script src="~/chameleon/theme-assets/js/core/app-lite.js" type="text/javascript"></script>
    <!-- END CHAMELEON  JS-->
    <!-- BEGIN PAGE LEVEL JS-->
    <!-- END PAGE LEVEL JS-->
    @RenderSection("Scripts", required: false)
</body>
</html>